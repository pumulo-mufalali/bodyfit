{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": []
    },
    "typecheck": {},
    "lint": {},
    "test": {
      "dependsOn": ["^build"],
      "cache": true
    }
  }
}
